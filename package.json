{
   "name": "brain-agriculture-service",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "license": "ISC",
   "devDependencies": {
      "@jest/globals": "^29.7.0",
      "@types/express": "^5.0.0",
      "@types/jest": "29.5.5",
      "@types/node": "^22.10.5",
      "@types/pg": "^8.11.10",
      "@types/pg-types": "^2.2.0",
      "@types/supertest": "^6.0.2",
      "@typescript-eslint/eslint-plugin": "6.21.0",
      "@typescript-eslint/parser": "6.21.0",
      "eslint": "8.56.0",
      "jest": "29.4.0",
      "supertest": "^7.0.0",
      "ts-jest": "29.1.0",
      "ts-node": "^10.9.2",
      "ts-node-dev": "^2.0.0",
      "typescript": "4.9.5"
   },
   "dependencies": {
      "body-parser": "^1.20.3",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.1",
      "dotenv": "^16.4.7",
      "express": "^4.21.2",
      "express-jsdoc-swagger": "^1.8.0",
      "multer": "^1.4.5-lts.1",
      "pg": "^8.13.1",
      "reflect-metadata": "^0.1.13",
      "routing-controllers": "^0.10.4",
      "swagger-generator-express": "^2.1.1",
      "tsconfig-paths": "^4.2.0",
      "typedi": "^0.10.0",
      "typeorm": "0.3.20",
      "typeorm-typedi-extensions": "^0.4.1",
      "uuid": "^11.0.3",
      "winston": "^3.17.0"
   },
   "scripts": {
      "build": "tsc",
      "start": "ts-node-dev --poll src/index.ts",
      "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
      "docker:run": "docker run -d -p 5435:5432 -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=12345678 -e POSTGRES_DB=agriculture --name brain-agriculture-service postgres",
      "docker:stop": "docker stop brain-agriculture-service",
      "lint": "eslint src/**/*.ts",
      "lint:format": "eslint src/**/*.ts --fix",
      "test:unit": "jest --logHeapUsage --runInBand --detectOpenHandles -c jest-unit-config.js",
      "test:unit:coverage": "jest --coverage --runInBand --detectOpenHandles --forceExit -c jest-unit-config.js",
      "test:integrated": "jest --runInBand --forceExit -c jest-integrated-config.js",
      "test:integrated:coverage": "jest --coverage --runInBand --forceExit -c jest-integrated-config.js"
   }
}
